@model IEnumerable<ReportWebApp01.Models.Steps>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employee.Nickname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Employee.Nickname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { EmployeeID = item.EmployeeID, ActDate = item.ActDate }) |
            @Html.ActionLink("Details", "Details", new { EmployeeID = item.EmployeeID, ActDate = item.ActDate }) |
            @Html.ActionLink("Delete", "Delete", new { EmployeeID = item.EmployeeID, ActDate = item.ActDate })
        </td>
    </tr>
}

</table>
